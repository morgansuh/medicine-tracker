QAlBT,EAkBS;UAAA;AAhBN,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,MAAA,CAAA,MAAA,IAAA,MAAA,CAAA,MAAA,OAAA,MAAA,YAAF;AAAA,KAAA,CAgBC;AAfP,IAAA,IAAI,EAAC,QAeE;AAdP,aAAM;AAcC,GAlBT,EAgBC,qBAhBD,C,qCAmBI,CAnVN,C,qCAoVI,CAlWN,C","sourcesContent":["<template>\n  <div class=\"max-w-screen-sm mx-auto px-4 py-10\">\n    <!-- App Msg -->\n    <div\n      v-if=\"statusMsg || errorMsg\"\n      class=\"mb-10 p-4 rounded-md shadow-md bg-light-grey\"\n    >\n      <p class=\"text-at-light-pink\">\n        {{ statusMsg }}\n      </p>\n      <p class=\"text-red-500\">\n        {{ errorMsg }}\n      </p>\n    </div>\n\n    <div v-if=\"dataLoaded\">\n      <!-- General Medication Info -->\n      <div\n        class=\"\n          flex flex-col\n          items-center\n          p-8\n          rounded-md\n          shadow-md\n          bg-light-grey\n          relative\n        \"\n      >\n        <div v-if=\"user\" class=\"flex absolute left-2 top-2 gap-x-2\">\n          <div\n            class=\"\n              h-7\n              w-7\n              rounded-full\n              flex\n              justify-center\n              items-center\n              cursor-pointer\n              bg-at-light-pink\n              shadow-lg\n            \"\n            @click=\"editMode\"\n          >\n            <img\n              class=\"h-3.5 w-auto\"\n              src=\"@/assets/images/pencil-light.png\"\n              alt=\"\"\n            />\n          </div>\n          <div\n            @click=\"deleteMedicine\"\n            class=\"\n              h-7\n              w-7\n              rounded-full\n              flex\n              justify-center\n              items-center\n              cursor-pointer\n              bg-at-light-pink\n              shadow-lg\n            \"\n          >\n            <img\n              class=\"h-3.5 w-auto\"\n              src=\"@/assets/images/trashcan.png\"\n              alt=\"\"\n            />\n          </div>\n        </div>\n        <!-- Liquid image -->\n        <img\n          v-if=\"data.medicineType === 'liquid'\"\n          src=\"@/assets/images/liquid.png\"\n          class=\"h-24 w-auto\"\n          alt=\"\"\n        />\n\n        <!-- Capsule image -->\n        <img\n          v-if=\"data.medicineType === 'capsule'\"\n          src=\"@/assets/images/capsule.png\"\n          class=\"h-24 w-auto\"\n          alt=\"\"\n        />\n\n        <!-- Inject image -->\n        <img\n          v-if=\"data.medicineType === 'inject'\"\n          src=\"@/assets/images/inject.png\"\n          class=\"h-24 w-auto\"\n          alt=\"\"\n        />\n\n        <!-- Topical image -->\n        <img\n          v-if=\"data.medicineType === 'topical'\"\n          src=\"@/assets/images/topical.png\"\n          class=\"h-24 w-auto\"\n          alt=\"\"\n        />\n\n        <!-- Other image -->\n        <img\n          v-if=\"data.medicineType === 'other'\"\n          src=\"@/assets/images/other.png\"\n          class=\"h-24 w-auto\"\n          alt=\"\"\n        />\n\n        <span\n          class=\"\n            mt-6\n            py-1.5\n            px-5\n            text-xs text-white\n            bg-at-light-pink\n            rounded-lg\n            shadow-md\n          \"\n        >\n          {{ data.medicineType }}\n        </span>\n\n        <div class=\"w-full mt-6\">\n          <input\n            v-if=\"edit\"\n            type=\"text\"\n            class=\"p-2 w-full text-gray-500 focus:outline-none\"\n            v-model=\"data.medicineName\"\n          />\n          <h1 v-else class=\"text-at-light-pink text-2xl text-center\">\n            {{ data.medicineName }}\n          </h1>\n        </div>\n      </div>\n\n      <!-- Medications -->\n      <div\n        class=\"\n          mt-10\n          p-8\n          rounded-md\n          flex flex-col\n          item-center\n          bg-light-grey\n          shadow-md\n        \"\n      >\n        <!-- Liquid -->\n        <div\n          v-if=\"data.medicineType === 'liquid'\"\n          class=\"flex flex-col gap-y-4 w-full\"\n        >\n          <div\n            class=\"flex flex-col gap-x-6 gap-y-2 relative sm:flex-row\"\n            v-for=\"(item, index) in data.medicine\"\n            :key=\"index\"\n          >\n            <div class=\"flex flex-2 flex-col md:w-1/3\">\n              <label\n                for=\"medicine-name\"\n                class=\"mb-1 text-sm text-at-light-pink\"\n              >\n                Medication\n              </label>\n              <input\n                id=\"medicine-name\"\n                v-if=\"edit\"\n                class=\"p-2 w-full text-gray-500 focus:outline-none\"\n                type=\"text\"\n                v-model=\"item.medicine\"\n              />\n              <p v-else>{{ item.medicine }}</p>\n            </div>\n            <div class=\"flex flex-1 flex-col\">\n              <label for=\"dosage\" class=\"mb-1 text-sm text-at-light-pink\">\n                Dosage\n              </label>\n              <input\n                v-if=\"edit\"\n                id=\"dosage\"\n                class=\"p-2 w-full text-gray-500 focus:outline-none\"\n                type=\"text\"\n                v-model=\"item.dosage\"\n              />\n              <p v-else>{{ item.dosage }}</p>\n            </div>\n            <div class=\"flex flex-1 flex-col\">\n              <label for=\"notes\" class=\"mb-1 text-sm text-at-light-pink\">\n                Notes\n              </label>\n              <input\n                v-if=\"edit\"\n                id=\"notes\"\n                class=\"p-2 w-full text-gray-500 focus:outline-none\"\n                type=\"text\"\n                v-model=\"item.notes\"\n              />\n              <p v-else>{{ item.notes }}</p>\n            </div>\n            <div class=\"flex flex-1 flex-col\">\n              <label for=\"weight\" class=\"mb-1 text-sm text-at-light-pink\">\n                Weight (LB's)\n              </label>\n              <input\n                v-if=\"edit\"\n                id=\"weight\"\n                class=\"p-2 w-full text-gray-500 focus:outline-none\"\n                type=\"text\"\n                v-model=\"item.weight\"\n              />\n              <p v-else>{{ item.weight }}</p>\n            </div>\n            <img\n              v-if=\"edit\"\n              @click=\"deleteMedicine(item.id)\"\n              class=\"absolute h-4 w-auto -left-5 cursor-pointer\"\n              src=\"@/assets/images/trashcan.png\"\n              alt=\"\"\n            />\n          </div>\n          <button\n            v-if=\"edit\"\n            @click=\"addMedication\"\n            type=\"button\"\n            class=\"\n              py-2\n              px-6\n              rounded-sm\n              self-start\n              text-sm text-white\n              bg-at-light-pink\n              duration-200\n              border-solid border-2 border-transparent\n              hover:border-at-light-pink hover:bg-white hover:text-at-light-pink\n            \"\n          >\n            Add Medication\n          </button>\n        </div>\n\n        <!-- Cardio -->\n        <div v-else class=\"flex flex-col gap-y-4 w-full\">\n          <div\n            class=\"flex flex-col gap-x-6 gap-y-2 relative sm:flex-row\"\n            v-for=\"(item, index) in data.medicine\"\n            :key=\"index\"\n          >\n            <div class=\"flex flex-2 flex-col md:w-1/3\">\n              <label for=\"cardioType\" class=\"mb-1 text-sm text-at-light-pink\">\n                Type\n              </label>\n              <select\n                id=\"cardioType\"\n                v-if=\"edit\"\n                class=\"p-2 w-full text-gray-500 focus:outline-none\"\n                type=\"text\"\n                v-model=\"item.cardioType\"\n              >\n                <option value=\"#\">Select Type</option>\n                <option value=\"run\">Runs</option>\n                <option value=\"walk\">Walk</option>\n              </select>\n              <p v-else>{{ item.cardioType }}</p>\n            </div>\n            <div class=\"flex flex-1 flex-col\">\n              <label for=\"distance\" class=\"mb-1 text-sm text-at-light-pink\">\n                Distance\n              </label>\n              <input\n                v-if=\"edit\"\n                id=\"distance\"\n                class=\"p-2 w-full text-gray-500 focus:outline-none\"\n                type=\"text\"\n                v-model=\"item.distance\"\n              />\n              <p v-else>{{ item.distance }}</p>\n            </div>\n            <div class=\"flex flex-1 flex-col\">\n              <label for=\"duration\" class=\"mb-1 text-sm text-at-light-pink\">\n                Duration\n              </label>\n              <input\n                v-if=\"edit\"\n                id=\"duration\"\n                class=\"p-2 w-full text-gray-500 focus:outline-none\"\n                type=\"text\"\n                v-model=\"item.duration\"\n              />\n              <p v-else>{{ item.duration }}</p>\n            </div>\n            <div class=\"flex flex-1 flex-col\">\n              <label for=\"pace\" class=\"mb-1 text-sm text-at-light-pink\">\n                Pace\n              </label>\n              <input\n                v-if=\"edit\"\n                id=\"pace\"\n                class=\"p-2 w-full text-gray-500 focus:outline-none\"\n                type=\"text\"\n                v-model=\"item.pace\"\n              />\n              <p v-else>{{ item.pace }}</p>\n            </div>\n            <img\n              @click=\"deleteMedicine(item.id)\"\n              v-if=\"edit\"\n              class=\"absolute h-4 w-auto -left-5 cursor-pointer\"\n              src=\"@/assets/images/trashcan.png\"\n              alt=\"\"\n            />\n          </div>\n          <button\n            @click=\"addMedicine\"\n            v-if=\"edit\"\n            type=\"button\"\n            class=\"\n              py-2\n              px-6\n              rounded-sm\n              self-start\n              text-sm text-white\n              bg-at-light-pink\n              duration-200\n              border-solid border-2 border-transparent\n              hover:border-at-light-pink hover:bg-white hover:text-at-light-pink\n            \"\n          >\n            Add Medication\n          </button>\n        </div>\n      </div>\n\n      <!-- Update -->\n      <button\n        v-if=\"edit\"\n        @click=\"update\"\n        type=\"button\"\n        class=\"\n          mt-10\n          py-2\n          px-6\n          rounded-sm\n          self-start\n          text-sm text-white\n          bg-at-light-pink\n          duration-200\n          border-solid border-2 border-transparent\n          hover:border-at-light-pink hover:bg-white hover:text-at-light-pink\n        \"\n      >\n        Update Medication\n      </button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, computed } from \"vue\";\nimport { supabase } from \"../supabase/init\";\nimport { useRoute, useRouter } from \"vue-router\";\nimport store from \"../store/index\";\nimport { uid } from \"uid\";\n\nexport default {\n  name: \"view-medicine\",\n  setup() {\n    // Create data / vars\n    const data = ref(null);\n    const dataLoaded = ref(null);\n    const errorMsg = ref(null);\n    const statusMsg = ref(null);\n    const route = useRoute();\n    const router = useRouter();\n    const user = computed(() => store.state.user);\n\n    // Get current Id of route\n    const currentId = route.params.medicineId;\n    // Get workout data\n    const getData = async () => {\n      try {\n        const { data: medicine, error } = await supabase\n          .from(\"medicines\")\n          .select(\"*\")\n          .eq(\"id\", currentId);\n        if (error) throw error;\n        data.value = medicine[0];\n        dataLoaded.value = true;\n        console.log(data.value);\n      } catch (error) {\n        errorMsg.value = error.message;\n        setTimeout(() => {\n          errorMsg.value = false;\n        }, 5000);\n      }\n    };\n    getData();\n    // Delete workout\n    const deleteWorkout = async () => {\n      try {\n        const { error } = await supabase\n          .from(\"medicine\")\n          .delete()\n          .eq(\"id\", currentId);\n        if (error) throw error;\n        router.push({ name: \"Home\" });\n      } catch (error) {\n        errorMsg.value = `Error: ${error.message}`;\n        setTimeout(() => {\n          errorMsg.value = false;\n        }, 5000);\n      }\n    };\n    // Edit mode\n    const edit = ref(null);\n    const editMode = () => {\n      edit.value = !edit.value;\n    };\n\n    // Add medicine\n    const addMedication = () => {\n      if (data.value.medicineType === \"liquid\") {\n        data.value.medicine.push({\n          id: uid(),\n          medicine: \"\",\n          dosage: \"\",\n          notes: \"\",\n        });\n        return;\n      }\n      if (data.value.medicineType === \"capsule\") {\n        data.value.medicine.push({\n          id: uid(),\n          medicine: \"\",\n          dosage: \"\",\n          notes: \"\",\n        });\n        return;\n      }\n      if (data.value.medicineType === \"inject\") {\n        data.value.medicine.push({\n          id: uid(),\n          medicine: \"\",\n          units: \"\",\n          notes: \"\",\n        });\n        return;\n      }\n      if (data.value.medicineType === \"topical\") {\n        data.value.medicine.push({\n          id: uid(),\n          medicine: \"\",\n          dose: \"\",\n          notes: \"\",\n        });\n        return;\n      }\n      if (data.value.medicineType === \"other\") {\n        data.value.medicine.push({\n          id: uid(),\n          medicine: \"\",\n          units: \"\",\n          notes: \"\",\n        });\n        return;\n      }\n    };\n    // Delete med\n    const deleteMedicine = (id) => {\n      if (data.value.medicine.length > 1) {\n        data.value.medicine = data.value.medicine.filter(\n          (medicine) => medicine.id !== id\n        );\n        return;\n      }\n      errorMsg.value =\n        \"Error: Cannot remove, need to at least have one medicine\";\n      setTimeout(() => {\n        errorMsg.value = false;\n      }, 5000);\n    };\n    // Update meds\n    const update = async () => {\n      try {\n        const { error } = await supabase\n          .from(\"medicine\")\n          .update({\n            medicineName: data.value.medicineName,\n            medicine: data.value.medicine,\n          })\n          .eq(\"id\", currentId);\n        if (error) throw error;\n        edit.value = false;\n        statusMsg.value = \"Success: Medication Updated!\";\n        setTimeout(() => {\n          statusMsg.value = false;\n        }, 5000);\n      } catch (error) {\n        errorMsg.value`Error: ${error.message}`;\n        setTimeout(() => {\n          errorMsg.value = false;\n        }, 5000);\n      }\n    };\n    return {\n      statusMsg,\n      data,\n      dataLoaded,\n      errorMsg,\n      edit,\n      editMode,\n      user,\n      deleteWorkout,\n      addMedication,\n      deleteMedicine,\n      update,\n    };\n  },\n};\n</script>\n"],"sourceRoot":""}]}